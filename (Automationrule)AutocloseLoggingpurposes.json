{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
        "workspace": {
            "type": "String"
        }
    },    
    "resources": [
        {
            "id": "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/AutomationRules/00000000-0000-1200-0000-000000000000')]",
            "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/00000000-0000-1200-0000-000000000000')]",
            "type": "Microsoft.OperationalInsights/workspaces/providers/automationRules",
            "apiVersion": "2021-10-01-preview",
            "properties": {
                "displayName": "[Autoclose] - Logging purposes",
                "order": 3,
                "triggeringLogic": {
                    "isEnabled": true,
                    "triggersOn": "Incidents",
                    "triggersWhen": "Created",
                    "conditions": [
                        {
                            "conditionType": "Property",
                            "conditionProperties": {
                                "propertyName": "IncidentTitle",
                                "operator": "Contains",
								"propertyValues": [
									"SharePointFileOperation via devices with previously unseen user agents",
									"SharePointFileOperation via previously unseen IPs",
									"Attempts to sign in to disabled accounts",
									"Exchange workflow MailItems Accessed operation anomaly",
									"Privileged Accounts - Sign in Failure Spikes",
									"Threatview.io",
									"Possible Emotet activity",
									"Guest Users Invited to Tenant by New Inviters",
									"Sign-ins from IPs that attempt sign-ins to disabled accounts (Uses Authentication Normalization)",
									"Conditional Access"
								]
                            }
                        }
                    ]
                },
      "actions": [
        {
          "order": 1,
          "actionType": "ModifyProperties",
          "actionConfiguration": {
            "severity": null,
            "status": "Closed",
            "classification": "BenignPositive",
            "classificationReason": "SuspiciousButExpected",
            "classificationComment": "Incidents created primarely for history purposes: OK",
            "owner": null,
            "labels": null
          }
        }
      ],
      "lastModifiedTimeUtc": "2022-11-04T11:50:51Z",
      "createdTimeUtc": "2022-11-04T11:50:51Z",
      "lastModifiedBy": {
        "objectId": "b4231591-b602-4132-aefa-a3dea77769fd",
        "email": "support@jdm.dk",
        "name": "SYSTEM",
        "userPrincipalName": "SYSTEM"
      },
      "createdBy": {
        "objectId": "b4231591-b602-4132-aefa-a3dea77769fd",
        "email": "support@jdm.dk",
        "name": "SYSTEM",
        "userPrincipalName": "SYSTEM"
      }
    }
  }
]
}

