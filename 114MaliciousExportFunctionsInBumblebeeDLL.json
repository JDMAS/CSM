                "techniques": [
                    "T1070",
                    "T1055",
                    "T1014",
                    "T1218",
                    "T1120",
                    "T1069",
                    "T1018",
                    "T1016",
                    "T1053",
                    "T1204",
                    "T1566",
                    "T1021",
                    "T1053"
                ],
